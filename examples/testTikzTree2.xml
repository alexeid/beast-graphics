<beast version='2.0'
       namespace='beast.core:beast.evolution.alignment:beast.core.util:beast.evolution.nuc:beast.evolution.operators:beast.evolution.sitemodel:beast.evolution.substitutionmodel:beast.evolution.likelihood'>

    <taxonset spec='TaxonSet' id='taxonset'>
        <taxon spec='Taxon' id='A'/>
        <taxon spec='Taxon' id='B'/>
        <taxon spec='Taxon' id='C'/>
        <taxon spec='Taxon' id='D'/>
    </taxonset>


    <tree spec='beast.util.TreeParser' id='tree'>
        <taxonset idref='taxonset'/>
        <input name='newick'>
            <![CDATA[
            (D:[&rate=0.02]3,((B:[&rate=0.02]1,A:[&rate=0.03]1):[&rate=0.02]1,C:[&rate=0.01]2):[&rate=0.01]1);
            ]]>
        </input>
    </tree>

    <tree spec='beast.util.TreeParser' id='tree2'>
        <taxonset spec='TaxonSet' id='taxonset2'>
            <taxon idref='A'/>
            <taxon idref='B'/>
            <taxon idref='C'/>
            <taxon idref='D'/>
        </taxonset>
        <input name='newick'>
            <![CDATA[
            ((A:[&rate=0.03]3,B:2):1,(C:1,D:7):1);
            ]]>
            <!--(((A:0.3,B:0.2):0.2,C:0.2):0.2,D:0.8); -->
        </input>
    </tree>

    <run spec='beast.app.draw.tree.TikzTreeGrid' fileName="testTikzTree2.tex" width="200" height="200" oneScale="true"
         pdflatexPath="/usr/texbin/pdflatex">

        <treeDrawing spec='beast.app.draw.tree.TreeDrawing' showLeafLabels="true"
                     lineThickness="1.25" labelOffset="10" showInternodeIntervals="true" branchLabels="&amp;rate">
            <tree idref="tree"/>
        </treeDrawing>
        <treeDrawing spec='beast.app.draw.tree.UnrootedTreeDrawing' showLeafLabels="true"
                     lineThickness="1.25" labelOffset="10" showInternodeIntervals="true" branchLabels="&amp;rate"
                     theta="0">
            <tree idref="tree2"/>
        </treeDrawing>
    </run>
</beast>

