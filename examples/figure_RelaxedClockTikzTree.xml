<beast version='2.0'
       namespace='beast.core:beast.evolution.alignment:beast.core.util:beast.evolution.nuc:beast.evolution.operators:beast.evolution.sitemodel:beast.evolution.substitutionmodel:beast.evolution.likelihood'>

    <taxonset spec='TaxonSet' id='taxonset'>
        <taxon spec='Taxon' id='A'/>
        <taxon spec='Taxon' id='B'/>
        <taxon spec='Taxon' id='C'/>
        <taxon spec='Taxon' id='D'/>
    </taxonset>


    <tree spec='beast.util.TreeParser' id='tree'>
        <taxonset idref='taxonset'/>
        <input name='newick'>
            <![CDATA[
            (D:[&rate=1]4.5,((B:[&rate=1]1,A:[&rate=1.5]1):[&rate=1]2,C:[&rate=0.5]3):[&rate=0.5]1.5);
            ]]>
        </input>
    </tree>

    <tree spec='beast.util.TreeParser' id='tree2'>
        <taxonset spec='TaxonSet' id='taxonset2'>
            <taxon idref='A'/>
            <taxon idref='B'/>
            <taxon idref='C'/>
            <taxon idref='D'/>
        </taxonset>
        <input name='newick'>
            (D:4.5,((B:1,A:1.5):2,C:1.5):0.75);
        </input>
    </tree>

    <run spec='beast.app.draw.tree.TikzTreeGrid' fileName="figure_RelaxedClockTikzTree.tex" width="150" height="150"
         oneScale="true"
         strideLength="2"
         isHorizontalStride="true" columnSpacer="60" pdflatexPath="/usr/texbin/pdflatex">

        <treeDrawing spec='beast.app.draw.tree.TreeDrawing' showLeafLabels="true"
                     lineThickness="1.25" labelOffset="10" showLeafTimes="true" showInternalNodeTimes="true"
                     branchLabels="&amp;rate"
                     orientation="down">
            <tree idref="tree"/>
        </treeDrawing>
        <treeDrawing spec='beast.app.draw.tree.TreeDrawing' showLeafLabels="true"
                     lineThickness="1.25" labelOffset="10" showLeafTimes="false" orientation="down">
            <tree idref="tree2"/>
        </treeDrawing>
        <!-- <treeDrawing spec='beast.app.draw.tree.UnrootedTreeDrawing' showLeafLabels="true"
                    lineThickness="1.25" labelOffset="10">
           <tree idref="tree2"/>
       </treeDrawing> -->

    </run>
</beast>

